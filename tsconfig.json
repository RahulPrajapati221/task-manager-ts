{
  "compilerOptions": {
    /* Language and Environment */
    "target": "ES6",

    /* Modules */
    "module": "commonjs",

    // "rootDir": "./",
    "moduleResolution": "node",
    "outDir": "./dist",
    "rootDir": "./src",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    /* Type Checking */
    "strict": false,

    /* Completeness */
    "skipLibCheck": true
  },
  "include": ["src/**/*"]
}
